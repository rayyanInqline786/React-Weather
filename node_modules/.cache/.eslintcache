ort default App;\n","/home/rayyan/Data-Drive/React-Weather/weather-app/src/reportWebVitals.js",[],"/home/rayyan/Data-Drive/React-Weather/weather-app/src/Component/UserInput.js",[],"/home/rayyan/Data-Drive/React-Weather/weather-app/src/Component/WeatherCard.js",["27","28"],"import React from 'react';\n\nconst WeatherCard = (props) => {\n    let { temp, humidity, feelsLike, main, description, city, country } = props;\n    temp = temp-273.15\n    temp = temp.toFixed(2)\n    feelsLike = feelsLike-273.15\n    feelsLike = feelsLike.toFixed(2)\n    return (\n        <div className=\"container mt-5\">\n            <div className=\"row\">\n                <div className=\"col-lg-6\">\n                    <h4>Temperature : {temp} </h4>\n                    <hr></hr>\n                    <h4>FeelsLike : {feelsLike}</h4>\n                    <hr></hr>\n                    <h4>Humidity : {humidity}</h4>\n                    <hr></hr>\n                    <h4>City : {city} </h4>\n                    <hr></hr>\n                    <h4>Country : {country} </h4>\n                    <hr></hr>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default WeatherCard;",{"ruleId":"29","severity":1,"message":"30","line":2,"column":26,"nodeType":"31","messageId":"32","endLine":2,"endColumn":35},{"ruleId":"29","severity":1,"message":"33","line":2,"column":37,"nodeType":"31","messageId":"32","endLine":2,"endColumn":45},{"ruleId":"34","severity":1,"message":"35","line":63,"column":7,"nodeType":"36","endLine":63,"endColumn":37},{"ruleId":"29","severity":1,"message":"37","line":4,"column":38,"nodeType":"31","messageId":"32","endLine":4,"endColumn":42},{"ruleId":"29","severity":1,"message":"38","line":4,"column":44,"nodeType":"31","messageId":"32","endLine":4,"endColumn":55},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'useState' is defined but never used.","jsx-a11y/alt-text","img elements must have an alt prop, either with mean